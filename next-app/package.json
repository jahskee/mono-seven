{
  "name": "7eleven-Pokemon",
  "version": "0.1.67",
  "private": true,
  "scripts": {
    "init-script": "chmod 775 bin/*",
    "bump-version": "npm version patch --force  && bin/bump-version.sh",
    "build": "next build",
    "start": "next dev -p 3000",
    "start-prod": "next build && next start",
    "docker-build": "next build && docker build -t jahskee/nextapp .",
    "docker-rm-containers": "docker rm -vf $(docker ps -a -q)",
    "docker-rm-images": "docker rmi -f $(docker images -a -q)",
    "docker-push": "yarn init-script && bin/docker-push.sh",
    "docker-run": "yarn docker-build && docker rm -f nextapp && docker run --name nextapp -p 0.0.0.0:3000:3000 jahskee/nextapp",
    "lint": "./node_modules/.bin/eslint --fix 'src/**/*.jsx' ",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/styles": "^4.11.4",
    "@storybook/addon-knobs": "^6.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "graphql": "^15.5.1",
    "lodash.debounce": "^4.0.8",
    "material-ui-search-bar": "^1.0.0",
    "next": "^10.0.0",
    "next-images": "^1.8.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "storybook-dark-mode": "^1.0.8",
    "ts-node": "^10.1.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-controls": "^6.3.4",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-viewport": "^6.3.4",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/react": "^3.4.12",
    "@testing-library/react": "^12.0.0",
    "@types/material-ui": "^0.21.9",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "react-test-render": "^1.1.2",
    "storybook-addon-apollo-client": "^4.0.8",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "typescript": "^4.3.5"
  }
}
